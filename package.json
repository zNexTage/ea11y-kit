  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "release-lib": "del core && mkdir core && cross-env NODE_ENV=production babel src/helper-components --out-dir core/helper-components --copy-files --ignore __tests__,spec.js,test.js,__snapshots__ && cross-env NODE_ENV=production babel src/exceptions --out-dir core/exceptions --copy-files --ignore __tests__,spec.js,test.js,__snapshots__ && cross-env NODE_ENV=production babel src/lib --out-dir core/lib --copy-files --ignore __tests__,spec.js,test.js,__snapshots__ && cross-env NODE_ENV=production babel src/stitches.config.js --out-dir core --copy-files --ignore __tests__,spec.js,test.js,__snapshots__ && cross-env NODE_ENV=production babel src/utils --out-dir core/utils --copy-files --ignore __tests__,spec.js,test.js,__snapshots__ && cross-env NODE_ENV=production babel ./package.json --out-dir core --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "build": "react-scripts build",
    "test": "react-scripts test --silent",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "main": "core/index.js",
  "module": "core/index.js",
  "files": [
    "core",
    "LICENCE.txt",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zNexTage/ea11y-kit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/react": "^4.9.1",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@chromatic-com/storybook": "^1.2.25",
    "@storybook/addon-a11y": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/react-webpack5": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "babel-cli": "^6.26.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-storybook": "^0.8.0",
    "file-loader": "^6.2.0",
    "react-router-dom": "^6.26.0",
    "storybook": "^8.0.4"
  }
}
